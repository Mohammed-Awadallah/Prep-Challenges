            ],
            "outputs": []
        },
        {
            "language": "javascript",
            "source": [
                "function equalizeArray(arr) {\n    let obj = {}\n    for (let i of arr ){\n        if(obj[i]){\n            obj[i]++\n        }else {\n            obj[i] = 1 \n        }\n    }\n   const value =  Object.values(obj)\n   const max = Math.max(...value);\n   const finalResult = arr.length - max;\n   return finalResult;\n}"
            ],
